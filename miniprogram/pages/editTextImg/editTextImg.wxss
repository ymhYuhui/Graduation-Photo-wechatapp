
.imageView{
  width:750rpx;
  text-align: center;
  position: relative;
  background-color: #fffdfd;
  overflow: hidden;
}

.imageView image{  
    position: absolute;
    display: block;
    margin: 0 auto;
}
.toolbar-bottom{
  display: flex;
  justify-content: space-around;
  width:100%;
  position: fixed;
  bottom: 0;
  height:160rpx;
  border-top: 1rpx solid #f7f7f7;
  background-color: #ffffff;
}
.toolbar-bottom view{
  display: inline-block;
  width:150rpx;
  height:120rpx;
  font-size: 28rpx;
  text-align: center;
  margin-top: 20rpx;
  position: relative;
  border-radius: 10rpx;
}
.toolbar-bottom view:active{
  background-color: rgba(255,255,255,0.5);
}
.toolbar-bottom view text{
  display: block;
  margin: 0 auto;
  color: #164da0;
}
.toolbar-bottom view .text{
  position: absolute;
  bottom: 0;
  left:50%;
  transform: translateX(-50%);
  white-space: nowrap;
  
}
.toolbar-top{
  width:100%;
  height:200rpx;
  position: absolute;
  bottom: 160rpx;
  background-color: #164da0;
  border-top: 1rpx solid #164da0;
  border-top-left-radius:20rpx; 
  border-top-right-radius:20rpx;
  animation-name:toolbar;
  animation-duration: 0.2s;
  animation-timing-function: ease;
}
.toolbar-width{
  display: flex;
  flex-direction: column;
  justify-content: space-around;
  align-items: center;
}
.slider-box{
  width:80%;
}
.color-box{
  width:750rpx;
  height:100%;
  display: flex;
  flex-direction: row;
  justify-content: space-around;
  flex-wrap :wrap;
  align-content: space-around;
}
.color-point{
  width:60rpx;
  height:60rpx;
  margin: 20rpx 30rpx;
  border-radius: 50%; 
  box-shadow: 0 0 0 4rpx transparent;
}
.color-point:active{
  box-shadow: 0 0 0 4rpx #fff;
}
.toolbar-back,.toolbar-font-style{
  display: flex;
  flex-direction: row;
  justify-content: space-around;
  align-items: center;
}
